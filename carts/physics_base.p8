version 34
__lua__
// constants
dt = 1.0 / 60.0
grav = 9.81
damp_x = 0.95
// player states
state_idl = 0
state_wlk = 1
state_ju = 2
state_jd = 3
state_lnd = 4


function _init()
	
	p = {}
	p.mov = 20.0
	p.face_r = true
	p.state = state_idl
	p.fc = 0
	// player position
	p.x = 64.0
	p.y = 64.0 
	// player speed
	p.dx = 0.0
	p.dy = 0.0
	// inputs
	inp = {}
	inp.left = false
	inp.right = false
	inp.jump = false
end
-->8
function _update60()
	update_inputs()
	update_vel()
	update_pos()
	collide()
	update_state()
	printh(p.state)
end

function update_inputs()
	inp.left = false
	inp.right = false
	if (not (btn(⬅️) and btn(➡️))) then
		if (btn(⬅️)) inp.left = true 
		if (btn(➡️)) inp.right = true
	end
end

function update_vel()
	//gravity
	p.dy += grav * dt
	// movement
	if (inp.left) p.dx = -p.mov
	if (inp.right) p.dx = p.mov
	// todo: clamp velocities
	// damping
	p.dx *= damp_x
end

function update_pos()
	// apply velocity	
	p.x += p.dx * dt
	p.y += p.dy * dt
end

function collide()
	// bonk with screen bounds
	// todo: replace with 
	// intelligent colliison
	if p.x < 0 then
		p.x = 0
		p.dx *= -1
	end
	
	if p.x > 127 then
		p.x = 127
		p.dx *= -1
	end
	
	if p.y < 0 then
		p.y = 0
		p.dy = 0
	end
	
	if p.y > 127 then
		p.y = 127
		p.dy = 0
	end
end

function update_state()
	// update frame count
	p.fc += 1
	// update facing direction
	if (p.face_r and inp.left) p.face_r=false
	if (not p.face_r and inp.right) p.face_r=true	
	
	// update player state
	if (p.state == state_idl) then
		if (inp.left or inp.right) then
			p.state = state_wlk
			p.fc = 0
		end
		return
	end
	if (p.state == state_wlk) then
		if (not inp.left and not inp.right) then
				p.state = state_idl
				p.fc = 0
		end
	end	
end
-->8
function _draw()
	cls(1)
	pset(flr(p.x), flr(p.y), 12)
end
